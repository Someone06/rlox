fun fib(n) {
  if(n < 2) return n;
  return fib(n - 2) + fib(n - 1);
}

fun fib2(n) {
    if(n < 2) {
        return n;
   } else {
        return fib(n - 2) + fib(n - 1);
   }
}

fun fib3(n) {
    var a = 0;
    var b = 1;

    while(n > 0) {
        var tmp = a + b;
        a = b;
        b = tmp;
        n = n - 1;
    }

    return a;
}

print fib(5);
print fib2(5);
print fib3(5);